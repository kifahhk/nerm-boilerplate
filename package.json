{
  "name": "NERM-Boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for building apps with Node / Express / React Redux and MongoDB",
  "scripts": {
    "test": "npm run set-test-env && npm run test:client && npm run test:server",
    "set-test-env": "cross-env NODE_ENV=test PORT=8080 MONGO_URL=mongodb://localhost:27017/nerm-db",
    "test:client": "node_modules/.bin/mocha --recursive --compilers js:babel-core/register --require ./client/util/test-helper.js --require ignore-styles client/**/*.spec.js",
    "test:server": "node_modules/.bin/mocha --recursive --compilers js:babel-core/register --require ./server/util/test-helper.js --require ignore-styles server/**/*.spec.js",
    "watch:test": "npm run test -- --watch",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production BABEL_DISABLE_CACHE=1 webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint:client": "eslint client",
    "lint:server": "eslint server",
    "lint": "eslint client server"
  },
  "pre-commit": [

  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kifahhk/nerm-boilerplate"
  },
  "bugs": {
